<Window x:Class="MaquetteDrumstik.PrintVideo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaquetteDrumstik"
        mc:Ignorable="d"
        Title="PrintVideo" Height="450" Width="800">
    <Grid Margin="10,0,2,-21" Height="auto" Width="auto">
        <MediaElement LoadedBehavior="Manual" UnloadedBehavior="Manual" Name="videoExo" HorizontalAlignment="Right" Height="auto" VerticalAlignment="Bottom" Width="auto" Margin="0,0,28,24"/>
        <ComboBox SelectionChanged="comboquality_SelectionChanged" Name="comboquality" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="79"/>

    </Grid>
</Window>
